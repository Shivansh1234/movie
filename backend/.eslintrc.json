{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": [
            "error",
            "always"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "indent": [
            "error",
            4
        ],
        "comma-dangle": [
            "error",
            "never"
        ],
        "prefer-destructuring": [
            "error",
            {
                "object": false,
                "array": false
            }
        ],
        "no-underscore-dangle": "off"
    }
}